<Window x:Class="RevitTimasBIMTools.Views.CutOpeningWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RevitTimasBIMTools.Views"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="CutOpeningWindows" Height="500" Width="900" 
        ResizeMode="CanResizeWithGrip" WindowStyle="None"
        Topmost="True" AllowsTransparency="True" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="400"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="700"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Row="0" Grid.Column="0" Margin="10">
            <local:CutOpeningView x:Name="Header" />
        </Grid>
    </Grid>
</Window>
